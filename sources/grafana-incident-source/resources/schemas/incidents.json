{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "incidentID",
    "labels",
    "isDrill",
    "createdTime",
    "modifiedTime",
    "closedTime",
    "durationSeconds",
    "status",
    "title",
    "slug",
    "overviewURL",
    "incidentStart",
    "incidentEnd",
    "version"
  ],
  "properties": {
    "incidentID": {
      "type": "string"
    },
    "severity": {
      "type": "string"
    },
    "labels": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "label"
        ],
        "properties": {
          "label": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "colorHex": {
            "type": "string"
          }
        }
      }
    },
    "isDrill": {
      "type": "boolean"
    },
    "createdTime": {
      "type": "string"
    },
    "modifiedTime": {
      "type": "string"
    },
    "closedTime": {
      "type": "string"
    },
    "durationSeconds": {
      "type": "integer"
    },
    "createdByUser": {
      "type": "object",
      "required": [
        "userID",
        "name"
      ],
      "properties": {
        "userID": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "status": {
      "type": "string"
    },
    "statusModifiedTime": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "overviewURL": {
      "type": "string"
    },
    "roles": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "role",
          "user"
        ],
        "properties": {
          "role": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "user": {
            "type": "object",
            "required": [
              "userID",
              "name",
              "email"
            ],
            "properties": {
              "userID": {
                "type": "string"
              },
              "name": {
                "type": "string"
              },
              "email": {
                "type": "string"
              }
            }
          }
        }
      }
    },
    "version": {
      "type": "number"
    },
    "summary": {
      "type": "string"
    },
    "incidentStart": {
      "type": "string"
    },
    "incidentEnd": {
      "type": "string"
    }
  }
}
